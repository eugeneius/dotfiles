[color]
	ui = auto
[core]
	editor = vim
[diff]
	algorithm = patience
	renames = true
	wordRegex = \\w+|[^[:space:]]
[fetch]
	prune = true
[merge]
	ff = only
[push]
	default = current
[rebase]
	autosquash = true
[stash]
	showPatch = true
[transfer]
	fsckObjects = true
[alias]
	review = "log --reverse --patch master@{upstream}..HEAD"
	fixup = "commit --amend --no-edit"
[include]
	path = ~/.gitconfig.local
