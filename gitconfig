[color]
	ui = auto
[core]
	editor = vim
[diff]
	algorithm = patience
	renames = true
	wordRegex = \\w+|[^[:space:]]
[fetch]
	prune = true
[merge]
	ff = only
[push]
	default = current
[alias]
	review = "log --reverse --patch origin/master..HEAD"
	fixup = "commit --amend --no-edit"
[include]
	path = ~/.gitconfig.local
